{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch TokenUtils",
      "request": "launch",
      "mainClass": "org.springframework.samples.ntfh.util.TokenUtils",
      "projectName": "LIng-2"
    },
    {
      "type": "java",
      "name": "Launch TokenValidator",
      "request": "launch",
      "mainClass": "org.springframework.samples.ntfh.util.TokenValidator",
      "projectName": "LIng-2"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "pwa-chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "java",
      "name": "Launch NtfhApplication",
      "request": "launch",
      "mainClass": "org.springframework.samples.ntfh.NtfhApplication",
      "projectName": "LIng-2"
    }
  ]
}
